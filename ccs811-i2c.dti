/dts-v1/;
/plugin/;
/ {
    compatible = "brcm,bcm2835";
    fragment@0 {
        target = <&i2c1>;
        __overlay__ {
            #address-cells = <1>;
            #size-cells = <0>;
            ccs811-i2c@5a {
            compatible = "ccs811";
            reg = <0x5a>;
            };
        };
    };
};